using System;
using System.Collections.Generic;

class AssigmentQuestion{

static void GetUniqueAndDuplicateWords(string input)
{
    var words = input.Split(' ');
    var uniqueWords = new HashSet<string>(words);
    var duplicateWords = new List<string>();

    foreach (var word in words)
    {
        if (uniqueWords.Contains(word))
        {
            uniqueWords.Remove(word);
        }
        else
        {
            duplicateWords.Add(word);
        }
    }

    // Console.WriteLine($"Unique: {string.Join(", ", uniqueWords)}");
    // Console.WriteLine($"Duplicate: {string.Join(", ", duplicateWords)}");
    var StringJoin=string.Join(", ", uniqueWords);
    var DuplicateString=string.Join(", ", duplicateWords);
    Console.WriteLine("Unique :"+string.Join(", ", uniqueWords));
    Console.WriteLine("Duplicate:"+DuplicateString);
}

static void Main(string[] args)
{
    GetUniqueAndDuplicateWords("Kusum is good");
}

}
